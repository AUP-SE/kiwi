<h1> All Courses:</h1>
<!-- = render "welcome/_index" -->
<table>
  <% @courses.each do |course| %>
    <tr>
    <td><%= course.title %></td>
    <td><%= course.coursecode %></td>
    <td><%= course.professor %></td>
    </tr>
<% end %>
</table>
<br>
<%= form_tag("/courses/search", method: "get") do %>
  <%= label_tag(:q, "Search Course:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>